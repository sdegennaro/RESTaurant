<header>
  <ul>
    <li><%= link_to "Home", profile_path%></li>
    <li><%= link_to "Menu", food_items_path %></li>
  </ul>
</header>
<h1>Log In Here</h1>

<%= form_tag '/sessions', method: :post do %>

  <%= label_tag :username %>
  <%= text_field_tag :username %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag "Log In" %>

<% end %>

<%= link_to "Create a new user", new_user_path %>
